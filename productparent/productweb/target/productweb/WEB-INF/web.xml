<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CreateProdcutServlet</servlet-name>
  	<display-name>CreateProdcutServlet</display-name>
  	<description></description>
  	<servlet-class>com.nihal.product.servlets.CreateProdcutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DisplayProductDetailsServlet</servlet-name>
  	<display-name>DisplayProductDetailsServlet</display-name>
  	<description></description>
  	<servlet-class>com.nihal.product.servlets.DisplayProductDetailsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreateProdcutServlet</servlet-name>
  	<url-pattern>/CreateProdcutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayProductDetailsServlet</servlet-name>
  	<url-pattern>/DisplayProductDetailsServlet</url-pattern>
  </servlet-mapping>
</web-app>
